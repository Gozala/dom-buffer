{
  "name": "dom-buffer",
  "version": "0.0.1",
  "description": "yet another virtual-dom implementation",
  "keywords": [
    "reflex",
    "react",
    "renderer"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/Gozala/dom-buffer",
  "main": "./lib/index.js",
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-plugin-remove-comments": "2.0.0",
    "babel-plugin-syntax-flow": "6.5.0",
    "babel-plugin-transform-es2015-modules-umd": "6.4.3",
    "babel-plugin-transform-flow-strip-types": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.4.3",
    "flow-bin": "0.24.0",
    "reflex": "0.2.0",
    "tap": "1.1.0",
    "tape": "4.4.0"
  },
  "scripts": {
    "test": "npm run flow && npm run tape",
    "tape": "tape -r babel-register test/**/*.js",
    "flow": "flow check",
    "start": "babel --watch --out-dir ./lib ./src",
    "build-node": "babel --out-dir ./lib ./src && cp ./src/*.flow ./lib",
    "build-browser": "mkdir -p dist && babel --out-file ./dist/reflex-virtual-dom-driver.js --plugins 'transform-es2015-modules-umd' ./src",
    "build": "npm run build-node && npm run build-browser",
    "prepublish": "npm run build && npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gozala/dom-buffer.git",
    "web": "https://github.com/Gozala/dom-buffer"
  },
  "bugs": {
    "url": "https://github.com/Gozala/dom-buffer/issues/"
  },
  "license": "MIT"
}
